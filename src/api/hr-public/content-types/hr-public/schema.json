{
  "kind": "collectionType",
  "collectionName": "hr_publics",
  "info": {
    "singularName": "hr-public",
    "pluralName": "hr-publics",
    "displayName": "HR Publikation",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "company": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company.company",
      "inversedBy": "hr_pubs"
    },
    "pub_date": {
      "type": "date",
      "required": true
    },
    "pub_summary": {
      "type": "string"
    },
    "pub_text": {
      "type": "richtext",
      "required": true
    },
    "pub_title": {
      "type": "string",
      "required": true
    },
    "pub_icon": {
      "type": "enumeration",
      "enum": [
        "faEllipsis",
        "faGraduationCap",
        "faUser",
        "faCirclePlus",
        "faBuilding",
        "faFile",
        "faCircleMinus",
        "faCodeBranch"
      ],
      "required": true,
      "default": "faEllipsis"
    },
    "dropdownLabel": {
      "type": "string",
      "required": false
    },
    "cert_documents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cert-document.cert-document",
      "mappedBy": "hr_id"
    }
  }
}
